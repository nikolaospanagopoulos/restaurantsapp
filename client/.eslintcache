talPrice: cart.totalPrice,\n        taxPrice: cart.taxPrice,\n      })\n    );\n  };\n  return (\n    <div className=\"order-info-page\">\n      <h1 className=\"delivery-payment-order-titles\">Place Order</h1>\n      <div className=\"checkout-steps\">\n        <CheckoutSteps step1 step2 step3 step4 />\n      </div>\n      <div className=\"order-info-container\">\n        <div>\n          <h2>Delivery</h2>\n          <p>\n            <strong>Address:</strong>\n            {cart.deliveryAddress.address}, {cart.deliveryAddress.city},{\" \"}\n            {cart.deliveryAddress.postalCode},\n          </p>\n        </div>\n        <div>\n          <h2>Payment Method</h2>\n          <strong>Method:</strong>\n          {cart.paymentMethod}\n        </div>\n      </div>\n      <h2 className=\"delivery-payment-order-titles\">Order Items</h2>\n      <div>\n        {cart.cartItems.length === 0 ? (\n          <Message>Your Cart Is Empty</Message>\n        ) : (\n          <div className=\"order-items-info\">\n            {cart.cartItems.map((item, index) => {\n              return (\n                <div key={index} className=\"order-items-info-container\">\n                  <div className=\"order-info-card\">\n                    <img src={item.image} alt={item.name} />\n                    <Link\n                      to={`/restaurants/${item.restaurant._id}/dishes`}\n                      className=\"dish-restaurant-link\"\n                    >\n                      <h5>{item.name} </h5>\n                    </Link>\n                    <h5>\n                      {item.qty}x{item.price} = €{item.qty * item.price}\n                    </h5>\n                  </div>\n                  <div></div>\n                </div>\n              );\n            })}\n          </div>\n        )}\n      </div>\n      <div>{error && <Message> {error} </Message>}</div>\n      <div className=\"sum-container\">\n        <div>\n          <h2>Your Sum</h2>\n\n          <h4>Price: {cart.itemsPrice}€</h4>\n          <h4>Tax: {cart.taxPrice}€</h4>\n          <h4>Total: {cart.totalPrice}€</h4>\n\n          <button\n            onClick={() => placeOrderHandler()}\n            disabled={cart.cartItems === 0}\n          >\n            Place Order\n          </button>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default PlaceOrderPage;\n","/home/nikos/Desktop/applications/restaurantsapp/client/src/Reducers/OrderReducers/OrderCreateReducers.js",[],"/home/nikos/Desktop/applications/restaurantsapp/client/src/Constants/OrderConstants/CreateOrderConstants.js",[],"/home/nikos/Desktop/applications/restaurantsapp/client/src/Actions/OrderActions/CreateOrderActions.js",[],"/home/nikos/Desktop/applications/restaurantsapp/client/src/Reducers/OrderReducers/OrderDetailsReducers.js",[],"/home/nikos/Desktop/applications/restaurantsapp/client/src/Constants/OrderConstants/OrderDetailsConstants.js",[],"/home/nikos/Desktop/applications/restaurantsapp/client/src/Pages/OrderPage/OrderPage.jsx",["338"],"import React, { useEffect, useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport Loader from \"../../Components/Loading/Loader\";\nimport Message from \"../../Components/Message/Message\";\nimport { Link } from \"react-router-dom\";\nimport { getOrderDetailsAction } from \"../../Actions/OrderActions/OrderDetailsActions\";\nimport { payOrder } from \"../../Actions/OrderActions/OrderPayActions\";\nimport { ORDER_PAY_RESET } from \"../../Constants/OrderConstants/OrderPayConstants\";\nimport \"./OrderPage.css\";\nimport axios from \"axios\";\nimport { PayPalButton } from \"react-paypal-button-v2\";\nconst OrderPage = ({ match }) => {\n  const orderId = match.params.id;\n  const dispatch = useDispatch();\n  const orderDetails = useSelector((state) => state.orderDetails);\n  const { loading, order, error } = orderDetails;\n  const [sdkReady, setSdkReady] = useState(false);\n\n  const orderPay = useSelector((state) => state.orderPay);\n  const { loading: loadingPay, success: successPay } = orderPay;\n\n\n  useEffect(() => {\n    const addPayPalScript = async () => {\n      const { data: clientId } = await axios.get(\"/api/v1/config/paypal\");\n      const script = document.createElement(\"script\");\n      script.type = \"text/javascript\";\n      script.src = `https://www.paypal.com/sdk/js?client-id=${clientId}`;\n      script.async = true;\n      script.onload = () => {\n        setSdkReady(true);\n      };\n      document.body.appendChild(script);\n    };\n    if (!order || order._id !== orderId || successPay) {\n      dispatch({ type: ORDER_PAY_RESET });\n      dispatch(getOrderDetailsAction(orderId));\n    } else if (!order.isPaid) {\n      if (!window.paypal) {\n        addPayPalScript();\n      } else {\n        setSdkReady(true);\n      }\n    }\n  }, [dispatch, order, orderId, successPay]);\n  const successPaymentHandler = (paymentResult) => {\n    console.log(paymentResult)\n    console.log(paymentResult)\n    dispatch(payOrder(orderId, paymentResult));\n  };\n  if (!loading) {\n    const addDecimals = (num) => {\n      return (Math.round(num * 100) / 100).toFixed(2);\n    };\n    //CALCULATE PRICES\n    order.itemsPrice = addDecimals(\n      order.orderItems.reduce((acc, item) => acc + item.price * item.qty, 0)\n    );\n  }\n\n  return loading ? (\n    <Loader />\n  ) : error ? (\n    <Message> {error} </Message>\n  ) : (\n    <div className=\"order-info-page\">\n      <h1 className=\"delivery-payment-order-titles\">Place Order</h1>\n\n      <div className=\"order-info-container\">\n        <div>\n          <h2>Delivery</h2>\n          <p>\n            <strong>Address:</strong>\n            {order.deliveryAddress.address}, {order.deliveryAddress.city},{\" \"}\n            {order.deliveryAddress.postalCode},\n          </p>\n        </div>\n        <div>\n          <h2>Payment Method</h2>\n          <strong>Method:</strong>\n          {order.paymentMethod}\n        </div>\n      </div>\n      {order.isPaid ? (\n        <Message color=\"green\"> order got paid at {order.paidAt} </Message>\n      ) : (\n        <Message> Not Paid </Message>\n      )}\n      {order.isDelivered ? (\n        <Message color=\"green\">\n          {\" \"}\n          order was delivered at {order.deliveredAt}{\" \"}\n        </Message>\n      ) : (\n        <Message> Not Yet Delivered </Message>\n      )}\n      <div className=\"order-user-info\">\n        <h2>User</h2>\n        <strong>Name:</strong>\n        {order.user.name}\n        <br />\n        <strong>Email:</strong>\n        <a href={`mailto:${order.user.email}`}>{order.user.email} </a>\n      </div>\n      <h2 className=\"delivery-payment-order-titles\">Order Items</h2>\n      <div>\n        {order.orderItems.length === 0 ? (\n          <Message>Your Cart Is Empty</Message>\n        ) : (\n          <div className=\"order-items-info\">\n            {order.orderItems.map((item, index) => {\n              return (\n                <div key={index} className=\"order-items-info-container\">\n                  <div className=\"order-info-card\">\n                    <img src={item.image} alt={item.name} />\n                    {/* <Link\n                      to={`/restaurants/${item.restaurant._id}/dishes`}\n                      className=\"dish-restaurant-link\"\n                    >\n                      <h5>{item.name} </h5>\n                    </Link> */}\n                    <h5>\n                      {item.qty}x{item.price} = €{item.qty * item.price}\n                    </h5>\n                  </div>\n                  <div></div>\n                </div>\n              );\n            })}\n          </div>\n        )}\n      </div>\n\n      <div className=\"sum-container\">\n        <div>\n          <h2>Your Sum</h2>\n\n          <h4>Price: {order.itemsPrice}€</h4>\n          <h4>Tax: {order.taxPrice}€</h4>\n          <h4>Total: {order.totalPrice}€</h4>\n        </div>\n        <div>\n          {!order.isPaid && (\n            <div>\n              {loadingPay && <Loader />}\n              {!sdkReady ? (\n                <Loader />\n              ) : (\n                <PayPalButton\n                  amount={order.totalPrice}\n                  onSuccess={successPaymentHandler}\n                />\n              )}\n            </div>\n          )}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default OrderPage;\n","/home/nikos/Desktop/applications/restaurantsapp/client/src/Actions/OrderActions/OrderDetailsActions.js",[],"/home/nikos/Desktop/applications/restaurantsapp/client/src/Reducers/OrderReducers/OrderPayReducer.js",[],"/home/nikos/Desktop/applications/restaurantsapp/client/src/Constants/OrderConstants/OrderPayConstants.js",[],"/home/nikos/Desktop/applications/restaurantsapp/client/src/Actions/OrderActions/OrderPayActions.js",[],"/home/nikos/Desktop/applications/restaurantsapp/client/src/Pages/OwnedRestaurants/OwnedRestaurants.jsx",["339"],"import React, { useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { getRestaurantList } from \"../../Actions/RestaurantActions/RestaurantListActions\";\nimport Message from \"../../Components/Message/Message\";\nimport Loader from \"../../Components/Loading/Loader\";\nimport Restaurant from '../../Components/Restaurant/Restaurant'\nconst OwnedREstaurants = ({ match }) => {\n  const userId = match.params.id;\n  const dispatch = useDispatch();\n  useEffect(() => {\n    dispatch(getRestaurantList('',userId));\n  }, [dispatch, userId]);\n\n  const restaurantList = useSelector((state) => state.restaurantList);\n  const { restaurants, loading, success, error } = restaurantList;\n  console.log(restaurants);\n  return (\n    <div>\n      <h1>Owned Restaurants</h1>\n      <div>\n        {loading ? (\n          <Loader />\n        ) : error ? (\n          <Message> {error} </Message>\n        ) : (\n          <div>\n            {restaurants.data.map((restaurant) => (\n  